{
  "manifest_version": 2,
  "name":"SD Prompts Checker",
  "version":"1.3",
  "icons":{
      "16":"16.png",
      "32":"32.png",
      "128":"128.png"
  },
  "background":{
      "scripts":["background.js"],
      "persistent": false
  },
  "content_scripts":[
      {
          "matches":[
              "<all_urls>"
          ],
          "js":[
              "content.js"
          ],
          "css":[
              "content.css"
          ]
      }
  ],
  "permissions":[
      "contextMenus",
      "https://*/*",
      "http://*/*"
  ]
}