{
  "manifest_version": 3,
  "name":"SD Prompts Checker",
  "version":"1.1",
  "icons":{
      "16":"16.png",
      "32":"32.png",
      "128":"128.png"
  },
  "background":{
      "service_worker":"background.js"
  },
  "content_scripts":[
      {
          "matches":[
              "<all_urls>"
          ],
          "js":[
              "content.js"
          ],
          "css":[
              "content.css"
          ]
      }
  ],
  "permissions":[
      "contextMenus",
      "http://*/*",
      "chrome.runtime"
  ]
}